{
 "compilerOptions": {
   "target": "ESNext",
   "lib": ["ESNext", "ES6", "DOM"],
   "esModuleInterop": true,
   "moduleResolution": "node",
   "emitDeclarationOnly": true,
   "declaration": true,
   "outDir": "dist",
   "skipLibCheck": true,
   "skipDefaultLibCheck": true
 },
  "include": [
    "src"
  ],
  "exclude": [
     "node_modules/**/*"
  ]
}
